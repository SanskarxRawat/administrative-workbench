{
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "build/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
          "src": "/api/(.*)",
          "dest": "api/index.js"
        },
        {
          "src": "/",
          "dest": "build/index.html"
        },
        {
          "src": "/(.+)",
          "dest": "build/$1"
        }
      ]
   
}